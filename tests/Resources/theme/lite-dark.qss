QWidget {
    outline: none;
}

QPushButton {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: #f2f2f2;
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: #1AFFFFFF;
}

QPushButton:pressed {
    color: rgb(191, 191, 191);
    background-color: #10FFFFFF;
}

QPushButton:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

QPushButton#btnNewTrack {
    image: url(:svg/icons/add_16_filled_white);
    padding: 6px;
    /* background-origin: content;
    background-position: left;
    background-repeat: no-repeat;
    text-align: right;
    padding-left: 22px; */
}

Button {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: #f2f2f2;
    padding: 6px 12px;
    /* width: 48px; */
}

Button:hover {
    background-color: #1AFFFFFF;
}

Button:pressed {
    color: rgb(191, 191, 191);
    background-color: #10FFFFFF;
}

Button:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

Button:checked {
    color: rgb(0, 28, 92);
    background: rgb(155, 186, 255);
}

Button:checked:hover {
    color: rgb(0, 28, 92);
    background: rgb(169, 196, 255);
}

Button:checked:pressed {
    color: rgba(0, 28, 92, 127);
    background: rgb(139, 175, 255);
}

Button:checked:disabled {
    color: rgb(166, 166, 166);
    background: rgb(205, 205, 205)
}

Button#CloseButton {
    border-radius: 0px;
    padding: 6px;
    /* image: url(:svg/title-bar/close.svg); */
    background: transparent;
}

Button#CloseButton:hover {
    background: #C42B1C;
}

Button#CloseButton:pressed {
    background: #B32E1D;
}

Button#MaximizeButton:checked {
    color: #f2f2f2;
}

Button#MinimizeButton,
Button#MaximizeButton {
    border-radius: 0px;
    padding: 6px;
    background: transparent;
}

Button#MinimizeButton:hover,
Button#MaximizeButton:hover {
    background: #10FFFFFF;
}

Button#MinimizeButton:pressed,
Button#MaximizeButton:pressed {
    background: #05FFFFFF;
}

AccentButton {
    background: rgb(155, 186, 255);
    border: none;
    border-radius: 6px;
    color: rgb(0, 28, 92);
    padding: 6px 12px;
}

AccentButton:hover {
    color: rgb(0, 28, 92);
    background-color: rgb(169, 196, 255);
}

AccentButton:pressed {
    color: rgba(0, 28, 92, 127);
    background-color: rgb(139, 175, 255);
}

AccentButton:disabled {
    color: rgb(166, 166, 166);
    background-color: rgb(205, 205, 205)
}

QComboBox {
    background: #10FFFFFF;
    border: none;
    border-radius: 6px;
    color: rgb(242, 242, 242);
    padding: 4px 8px;
}

QComboBox:hover {
    background-color: #1AFFFFFF;
}

QComboBox:disabled {
    color: rgb(127, 127, 127);
    background-color: rgb(64, 64, 64)
}

QComboBox QAbstractItemView {
    outline: 0px;
    border: 1px solid #80606060;
    color: #F0F0F0;
    background-color: #202122;
    border: 1px solid #606060;
    selection-background-color: #1AFFFFFF;
    border-radius: 6px;
    padding: 4px;
}

QComboBox QAbstractItemView:item {
    padding: 3px;
    border-radius: 4px;
    border: none;
}

QComboBox QAbstractItemView::item:selected {
    color: #F0F0F0;
    background-color: #1AFFFFFF;
}

QComboBox::drop-down {
    border: none
}

QComboBox::down-arrow {
    right: 8px;
    width: 12px;
    height: 12px;
    image: url(:svg/icons/chevron_down_16_filled_white.svg)
}

QScrollBar {
    background-color: transparent;
    border-style: none;
}

QScrollBar::add-page,
QScrollBar::sub-page {
    background: none;
}

QScrollBar::add-line,
QScrollBar::sub-line {
    width: 0px;
    height: 0px;
    border: none;
}

QScrollBar::vertical {
    width: 16px;
}

QScrollBar::handle::vertical {
    border-radius: 2px;
    margin: 6px;
    background: rgba(255, 255, 255, 0.25);
}

QScrollBar::handle::vertical::hover {
    border-radius: 3px;
    margin: 5px;
    background: rgba(255, 255, 255, 0.35);
}

QScrollBar::handle::vertical:pressed {
    background: rgba(255, 255, 255, 0.15);
}


QScrollBar::horizontal {
    height: 16px;
}

QScrollBar::handle::horizontal {
    border-radius: 2px;
    margin: 6px;
    background: rgba(255, 255, 255, 0.25);
}

QScrollBar::handle::horizontal::hover {
    border-radius: 3px;
    margin: 5px;
    background: rgba(255, 255, 255, 0.35);
}

QScrollBar::handle::horizontal:pressed {
    background: rgba(255, 255, 255, 0.15);
}

QSplitter {
    background-color: transparent;
    border: none;
    height: 6px
}

QSplitter::handle:vertical {
    height: 6px;
    background-color: transparent;
}

QListWidget {
    color: #F0F0F0;
    outline: 0px;
    background: transparent;
    border: none
}

QListWidget::item {
    border: none;
    border-radius: 6px;
    padding: 4px 8px;
    /* margin-top: 2px;
    margin-bottom: 2px; */
    color: #F0F0F0;
}

QListWidget::item:hover {
    background-color: #10FFFFFF;
}

QListWidget::item:selected {
    /* background-color: #1AFFFFFF; */
    background-color: #9BBAFF;
    color: #000;
}

